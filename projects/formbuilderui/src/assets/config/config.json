      "vertical": {
        "name": "home_slider_vertical",
        "htmlType": "checkbox",
        "validators": { "required": false }
      },
      "inverted": {
        "name": "home_slider_inverted",
        "htmlType": "checkbox",
        "validators": { "required": false }
      }
